@using Framework.MVC.Resources
@using Topppro.Entities.Resources

@model Topppro.Entities.Download

@using (Ajax.BeginForm("Edit", "Download", null,
        new AjaxOptions()
        {
            OnSuccess = "$('#download-list').dataTable().fnUpdate(data, el[0]); $('.bootstrap-dialog').modal('hide');"
        }))
{
    @Html.ValidationSummary(true, null, new { @class = "alert alert-danger" })
    <fieldset>
        @Html.EditorForModel()
    </fieldset>
}

@model Topppro.Entities.Package

@using (Ajax.BeginForm("Create", "Package", null,
        new AjaxOptions()
        {
            OnSuccess = "$('#package-list').dataTable().fnAddData(data); $('.bootstrap-dialog').modal('hide');"
        }))
{
    @Html.ValidationSummary(true, null, new { @class = "alert alert-danger" })
    <fieldset>
        @Html.EditorForModel()
    </fieldset>
}

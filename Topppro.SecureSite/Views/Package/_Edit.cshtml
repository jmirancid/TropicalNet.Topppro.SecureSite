@model Topppro.Entities.Package

@using (Ajax.BeginForm("Edit", "Package", null,
        new AjaxOptions()
        {
            OnSuccess = "$('#package-list').dataTable().fnUpdate(data, el[0]); $('.bootstrap-dialog').modal('hide');"
        }))
{
